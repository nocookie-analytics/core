{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.8.3",
    "perfume.js": "^5.3.0",
    "whatwg-fetch": "^3.4.1"
  },
  "devDependencies": {
    "@types/object-hash": "^1.3.1",
    "babel-core": "7.0.0-bridge.0",
    "flow-bin": "^0.142.0",
    "object-hash": "^2.1.1",
    "parcel-bundler": "^1.12.4",
    "tslint": "^6.1.2",
    "typescript": "^4.1.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "since 2017-06"
  ],
  "scripts": {
    "dev": "parcel index.html"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
